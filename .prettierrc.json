{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 600,
  "trailingComma": "none",
  "htmlWhitespaceSensitivity": "ignore",
  "bracketSameLine": true,
  "include": "src/**/*.{js,jsx,ts,tsx,css,scss,less,html,json,md}"
}
